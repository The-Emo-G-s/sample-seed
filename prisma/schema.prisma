// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Emoji {
  id Int @id @default(autoincrement())
  emoji String
  department String
  imageUrl String
  aColor String
  bColor String
  cColor String
  dColor String
  eColor String
}
